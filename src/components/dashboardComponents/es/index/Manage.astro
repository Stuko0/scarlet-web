<script>
    document.querySelectorAll('[data-tab]').forEach(function (item) {
        const activeTab = document.querySelector('[data-tab="order"][data-tab-page="active"]');
        if (activeTab) {
            activeTab.classList.add('bg-orange-600', 'text-white');
            activeTab.classList.remove('hover:text-gray-600');
        }
        if(item instanceof HTMLElement){
            item.addEventListener('click', function (e) {
                e.preventDefault();
                const tab = item.dataset.tab;
                const page = item.dataset.tabPage;
                const target = document.querySelector('[data-tab-for="' + tab + '"][data-page="' + page + '"]')
                document.querySelectorAll('[data-tab="' + tab + '"]').forEach(function (i) {
                    i.classList.remove('active', 'bg-orange-600', 'text-white');
                })
                document.querySelectorAll('[data-tab-for="' + tab + '"]').forEach(function (i) {
                    i.classList.add('hidden');
                })
                item.classList.add('active', 'bg-orange-600', 'text-white');
                item.classList.remove('hover:text-gray-600');
                target.classList.remove('hidden');
            })
        }
})
</script>